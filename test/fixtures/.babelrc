{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "react-intl": {
      "plugins":[
        ["react-intl-auto",
          {"removePrefix": "test/fixtures/"}
        ]
      ]
    }
  }
}
